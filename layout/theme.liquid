    align-items: center;
    gap: 10px;
  }

  .dev-banner__icon {
    font-size: 16px;
    line-height: 1;
    flex-shrink: 0;
  }

  .dev-banner__text {
    line-height: 1.5;
  }

  #dev-banner a {
    color: inherit;
    text-decoration: underline;
    font-weight: 600;
  }

  @media (max-width: 480px) {
    #dev-banner {
      font-size: 13px;
    }
    .dev-banner__inner {
      padding: 8px 12px;
    }
  }
</style>


