<!-- CONDITIONS GÉNÉRALES POPUP — TEXT 100% IDENTICAL -->
<dialog id="cg-dialog" style="
  border:none;
  border-radius:12px;
  width:90%;
  max-width:900px;
  max-height:85vh;
  padding:0;
">

